
{
  "version": 2,
  "builds": [{
    "src": "Meets/asgi.py",
    "use": "@vercel/python",
    "config": { "runtime": "python3.9" }
  }],
  "routes": [
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { 
      "src": "/(.*)", 
      "dest": "Meets/asgi.py",
      "headers": { "Content-Type": "text/html" } 
    }
  ]
}