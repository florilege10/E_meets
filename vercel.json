{
  "version": 2,
  "builds": [
    {
      "src": "Meets/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "wsgi": {
          "app": "Meets.wsgi.application"
        },
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "Meets/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "False"
  }
}
